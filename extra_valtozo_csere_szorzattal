
#include <stdlib.h>
#include <string>
#include <iostream>


using namespace std;

int main()
{
	//1. Cseréld fel két változó értékét segédváltozó nélkül.

	int a = 8;
	int b = 1;
	cout<<"Elotte : "<< a << " "<< b << endl;
	a = a * b;
	b = a / b;
	a = a / b;

	cout <<"Utana : "<< a << " " << b << endl;
} 
