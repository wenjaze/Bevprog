void swap_r(int& a, int& b)
{
    int temp;
    temp = a;
    a = b;
    b = temp;
}


int main()
{
    int x = 7;
    int y = 9;
    
    swap_r(x, y);    // OK

    cout << "x is now " << x << " and y is now " << y << '\n';

    const int cx = 7;
    const int cy = 9;



 

    double dx = 7;
    double dy = 9;

    return 0;
